{% set postslist = collections.posts %}

<!-- <p>Look behind you, a three-headed monkey.</p> -->

{% set postslist = collections.posts %}



<ul class="postlist">
  {% for post in postslist | reverse %}
    {% if post.data.title != "draft" %}
    <li class="postlist-item{% if post.url == url %} postlist-item-active{% endif %}">
       <a href="{{ post.url | url }}" class="postlist-link purple-text"> {% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a>
       <i style="font-weight:300!important">{{ post.data.date | readableDate() }}</i>
    </li>
    {% endif %}
  {% endfor %}
</ul>